{
  "name": "ruleiq",
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/health",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "PYTHON_ENV": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "api/main.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}