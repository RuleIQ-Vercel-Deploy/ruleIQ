{
  "name": "ruleiq-frontend",
  "version": "1.0.0",
  "description": "Enterprise-grade frontend for ruleIQ - AI-powered compliance automation platform for UK SMBs",
  "author": "ruleIQ Team",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:turbo": "next dev --turbo -p 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:staging": "NODE_ENV=production next build",
    "build:production": "NODE_ENV=production next build && pnpm cleanup",
    "cleanup": "tsx scripts/cleanup-production.ts",
    "start": "next start",
    "start:production": "NODE_ENV=production next start -p 3001",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:keys": "vitest __tests__/duplicate-keys.test.tsx",
    "test:ci": "vitest --run --coverage",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "migrate:colors": "tsx scripts/migrate-colors.ts --live",
    "migrate:colors:dry": "tsx scripts/migrate-colors.ts",
    "test:theme": "vitest tests/theme-migration.test.tsx",
    "deploy:prod": "tsx scripts/deploy-production.ts",
    "deploy:staging": "tsx scripts/deploy-production.ts --staging",
    "deploy:dry": "tsx scripts/deploy-production.ts --dry-run",
    "typecheck": "tsc --noEmit",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report test-results/e2e-report",
    "test:memory-leaks": "vitest run --reporter=verbose tests/**/*memory-leak*.test.tsx",
    "test:memory-leaks:watch": "vitest --reporter=verbose tests/**/*memory-leak*.test.tsx",
    "test:memory-leaks:report": "tsx tests/run-memory-leak-tests.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:smoke": "playwright test tests/e2e/smoke.test.ts",
    "test:e2e:accessibility": "playwright test tests/e2e/accessibility.test.ts",
    "qa:health-check": "tsx scripts/qa-health-check.ts",
    "qa:pr-analysis": "tsx scripts/qa-pr-analyzer.ts",
    "qa:affected-tests": "tsx scripts/qa-affected-tests.ts",
    "qa:flaky-detector": "tsx scripts/qa-flaky-detector.ts",
    "qa:quality-dashboard": "tsx scripts/qa-quality-dashboard.ts",
    "qa:performance-monitor": "tsx scripts/qa-performance-monitor.ts",
    "qa:a11y-tracker": "tsx scripts/qa-a11y-tracker.ts",
    "qa:morning-check": "pnpm qa:health-check && pnpm qa:flaky-detector && pnpm qa:quality-dashboard",
    "qa:pr-workflow": "pnpm qa:pr-analysis && pnpm qa:affected-tests",
    "qa:quality-gates": "pnpm qa:performance-monitor --enforce-gates && pnpm qa:a11y-tracker audit",
    "test:performance": "playwright test tests/performance/performance.test.ts",
    "test:visual": "playwright test --config=playwright.visual.config.ts",
    "test:visual:update": "UPDATE_SNAPSHOTS=true playwright test --config=playwright.visual.config.ts",
    "test:visual:components": "playwright test tests/visual/component-snapshots.test.tsx --config=playwright.visual.config.ts",
    "test:visual:pages": "playwright test tests/visual/page-layouts.test.tsx --config=playwright.visual.config.ts",
    "test:accessibility": "vitest tests/accessibility/accessibility.test.tsx",
    "test:critical-fixes": "vitest tests/critical-fixes/ --run",
    "test:critical-fixes:watch": "vitest tests/critical-fixes/",
    "test:critical-fixes:e2e": "playwright test tests/critical-fixes/complete-auth-flow.e2e.test.ts",
    "test:all": "pnpm test --run && pnpm test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "test:storybook:visual": "test-storybook --url http://localhost:6006",
    "chromatic": "chromatic --project-token=chpt_your_token_here",
    "analyze:bundle": "node scripts/bundle-analyzer.js",
    "analyze:bundle:ci": "pnpm build && pnpm analyze:bundle",
    "prepare": "husky install",
    "ci": "pnpm lint && pnpm typecheck && pnpm test --run && pnpm build",
    "preview": "pnpm build && pnpm start"
  },
